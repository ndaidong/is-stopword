{
  "version": "1.0.0",
  "name": "is-stopword",
  "description": "To check if a word is stopword in English",
  "homepage": "https://www.npmjs.com/package/is-stopword",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/is-stopword"
  },
  "author": "@ndaidong",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "lint": "eslint .",
    "nsp": "nsp check --output checkstyle",
    "tape": "nyc tape test/main.js | tap-spec",
    "report": "nyc report --reporter=lcov",
    "pretest": "npm run lint",
    "test": "npm run tape",
    "posttest": "npm run nsp && npm run report",
    "coveralls": "npm test && cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "bellajs": "latest",
    "coveralls": "latest",
    "eslint": "latest",
    "eslint-config-ggc": "latest",
    "nsp": "latest",
    "nyc": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "rating",
    "vote",
    "poll",
    "average",
    "score",
    "Wilson Score",
    "util"
  ],
  "license": "MIT"
}
